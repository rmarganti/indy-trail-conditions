{
    "extends": [
        "plugin:prettier/recommended",
        "prettier/@typescript-eslint",
        "react-app"
    ],
    "plugins": [
        "react",
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "tabWidth": 4,
                "trailingSpace": "es5"
            }
        ]
    },
    "parser": "@typescript-eslint/parser"
}